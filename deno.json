{
  "fmt": {
    "lineWidth": 120,
    "include": ["src/"]
  },
  "imports": {
    "dotenv": "https://deno.land/std@0.213.0/dotenv/mod.ts",
    "path": "https://deno.land/std@0.213.0/path/mod.ts",

    "discord.js": "npm:discord.js@14.14.1",
    "ioredis": "npm:ioredis@5.3.2",
    "openai": "npm:openai@4.25.0",
    "openai/resources": "npm:openai@4.25.0/resources/index.ts",

    "@/": "./src/",
    "@builders/": "./src/builders/",
    "@constants/": "./src/constants/",
    "@managers/": "./src/managers/",
    "@modules/": "./src/modules/",
    "@services/": "./src/services/",
    "@utilities/": "./src/utilities/"
  },
  "tasks": {
    "dev": "deno run -A --watch main.ts",
    "start": "deno run --allow-read, --allow-net --allow-env main.ts"
  }
}
