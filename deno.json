{
  "fmt": {
    "lineWidth": 120,
    "include": ["app/"]
  },
  "imports": {
    "dotenv": "https://deno.land/std@0.213.0/dotenv/mod.ts",
    "path": "https://deno.land/std@0.213.0/path/mod.ts",

    "discord.js": "npm:discord.js@14.14.1",
    "ioredis": "npm:ioredis@5.3.2",
    "openai": "npm:openai@4.25.0",
    "openai/resources": "npm:openai@4.25.0/resources/index.ts",

    "@/": "./app/",
    "@builders/": "./app/builders/",
    "@constants/": "./app/constants/",
    "@managers/": "./app/managers/",
    "@modules/": "./app/modules/",
    "@services/": "./app/services/",
    "@utilities/": "./app/utilities/"
  },
  "tasks": {
    "dev": "deno run -A --watch app/main.ts",
    "start": "deno run --allow-read, --allow-net --allow-env app/main.ts"
  }
}
